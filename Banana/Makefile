ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = ../bin/target/Debug/Banana
  TARGET = $(TARGETDIR)/libBanana.a
  OBJDIR = ../bin/obj/Debug/Banana
  PCH = incs.h
  GCH = $(OBJDIR)/$(notdir $(PCH)).gch
  DEFINES += -DMACRO_GLFW -DMACRO_OPENGL -DBUILD_DEBUG
  INCLUDES += -Ivendor/GLFW/include -Ivendor/GLAD/include/glad -Ivendor/IMGUI -Ivendor/GLM/glm -Ivendor/STB -I. -Isrc -Isrc/generic
  FORCE_INCLUDE += -include $(OBJDIR)/$(notdir $(PCH))
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -g
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -g
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += ../bin/target/Debug/GLAD/libGLAD.a ../bin/target/Debug/IMGUI/libIMGUI.a ../bin/target/Debug/GLFW/libGLFW.a
  LDDEPS += ../bin/target/Debug/GLAD/libGLAD.a ../bin/target/Debug/IMGUI/libIMGUI.a ../bin/target/Debug/GLFW/libGLFW.a
  ALL_LDFLAGS += $(LDFLAGS)
  LINKCMD = $(AR) -rcs "$@" $(OBJECTS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: prebuild prelink $(TARGET)
	@:

endif

