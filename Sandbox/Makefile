  LDDEPS += ../bin/target/Release/Banana/libBanana.a ../bin/target/Release/GLAD/libGLAD.a ../bin/target/Release/GLFW/libGLFW.a ../bin/target/Release/IMGUI/libIMGUI.a
  ALL_LDFLAGS += $(LDFLAGS) -s
